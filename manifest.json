{
    "manifest_version": 2,
    "name": "Persuasive Password Strength Indicator",
    "version": "1.0",
    "description": "An extension that alerts when the password box is clicked on",

    "permissions": [
      "activeTab","storage", "https://haveibeenpwned.com/*"
    ],

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"],
        "css": ["style.css"]
      }
    ]
  }